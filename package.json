{
  "name": "bobbycookbook",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:prettier",
    "lintfix": "prettier --write --list-different .",
    "watch:css": "tailwindcss -o assets/css/main.css --watch",
    "develop": "concurrently \"npm run dev\" \"npm run watch:css\""
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "core-js": "^3.19.3",
    "firebase": "^9.6.2",
    "nuxt": "^2.15.8",
    "tailwindcss-textshadow": "^2.1.3",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@nuxt/postcss8": "^1.1.3",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.12"
  }
}
